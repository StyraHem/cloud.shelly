{
	"name": "http-auth-client",
	"description": "Client side HTTP Authorization header handling",
	"version": "0.4.1",
	"main": "auth.js",
	"dependencies": {},
	"devDependencies": {
		"chai": "^4.2.0",
		"eslint": "^6.1.0",
		"mocha": "^6.2.0",
		"nyc": "^14.1.1"
	},
	"scripts": {
		"pretest": "eslint .",
		"test": "nyc --reporter=text --reporter=lcov mocha"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"author": "Fozi",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/fozi/http-auth-client"
	},
	"keywords": [
